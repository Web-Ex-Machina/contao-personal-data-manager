{
    "name": "webexmachina/personal-data-manager",
    "description": "Personal Data Manager bundle for Contao Open Source CMS",
    "keywords": ["webexmachina", "bundle", "contao","personal","rgpd","gpdr"],
    "type": "contao-bundle",
    "license": "LGPL-3.0+",
    "website": "https://www.webexmachina.fr/",
    "authors": [
    {
        "name": "Web ex Machina",
        "homepage": "https://www.webexmachina.fr",
        "role": "Developer"
    }],
    "support":
    {
        "issues": "https://github.com/webexmachina/personal-data-manager/issues",
        "source": "https://github.com/webexmachina/personal-data-manager"
    },
    "require":
    {
        "php": ">=7.4",
        "contao/core-bundle": "~4.13",
        "webexmachina/contao-utils": "^0.3",
        "plenta/contao-encryption ": "^2.1"
    },
    "require-dev":
    {
        "phpunit/phpunit": "^9.0",
        "codeception/codeception": "^4",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-webdriver": "^2.0",
        "contao/test-case":"~4"
    },
    "autoload":
    {
        "psr-4":
        {
            "WEM\\PersonalDataManagerBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "extra":
    {
        "contao-manager-plugin": "WEM\\PersonalDataManagerBundle\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao/manager-plugin": true,
            "composer/package-versions-deprecated": true,
            "contao-community-alliance/composer-plugin": true
        }
    }
}
